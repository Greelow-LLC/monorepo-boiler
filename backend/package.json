{
  "name": "backend",
  "version": "1.0.0",
  "description": "voyage backend boiler",
  "main": "index.js",
  "dependencies": {
    "aws-sdk": "^2.1225.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^6.0.0",
    "express-list-endpoints": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "puppeteer": "^15.5.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/express-jwt": "^6.0.4",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/jest": "^29.1.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "express-list-endpoints": "^5.0.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.7",
    "shx": "^0.3.4",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "ttypescript": "^1.5.13",
    "typescript-transform-paths": "^3.3.1"
  },
  "scripts": {
    "test": "docker ps -aq | xargs -r docker stop | xargs -r docker rm && docker volume ls -qf dangling=true | xargs -r docker volume rm && docker-compose -f 'docker-compose.yml' up -d && sleep 5 && jest -c ./jest.unit.json --maxWorkers=1",
    "test:coverage": "jest --coverage -c ./jest.unit.json",
    "start": "node build/index.js",
    "start:prod": "NODE_ENV=production && yarn build && yarn start",
    "dev": "NODE_PATH=src tsc-watch --noClear -p ./tsconfig.json --onSuccess \"ts-node src/index.ts\"",
    "develop": "yarn db:stop && yarn db:start && yarn dev",
    "clean": "shx rm -rf build",
    "build": "yarn clean && ttsc --project ./",
    "typeorm": "NODE_PATH=src typeorm-ts-node-esm -d src/ormconfig",
    "db:start": "docker-compose -f 'docker-compose.yml' up -d",
    "db:stop": "docker kill $(docker ps -q) || true",
    "db:clean": "docker rm $(docker ps -a -q) && docker volume prune -f",
    "db:seed": "NODE_PATH=src npx typeorm-ts-node-esm migration:create src/migrations/seed",
    "db:generate": "yarn typeorm migration:generate src/migrations/migrations && yarn build",
    "db:up": "yarn typeorm migration:run",
    "db:down": "yarn typeorm migration:revert",
    "db:drop": "yarn typeorm schema:drop"
  },
  "engines": {
    "node": "16.17.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node"
  }
}
